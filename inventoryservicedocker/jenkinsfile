#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./inventoryservicedocker clean install -DskipTests"
         
         
       }
   
   }
   
   stage("docker"){
    
      steps{
       bat "docker build -f inventoryservicedocker/dockerfile -t invapp-v2 ./inventoryservicedocker"
       bat "cd inventoryservicedocker & docker-compose up -d"
      
      }
   }
 
 }
 
 
 
}
